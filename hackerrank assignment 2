import java.util.Scanner;

public class MaximumElement {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();

        int b = scanner.nextInt();

        int max = Math.max(a, b);
        System.out.println("" + max);
    }
}


**

import java.util.Scanner;

public class TriangleValidityChecker {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int angleA = scanner.nextInt();

        int angleB = scanner.nextInt();

        int angleC = scanner.nextInt();

        boolean isValid = isTriangleValid(angleA, angleB, angleC);

        if (isValid) {
            System.out.println(1);
        } else {
            System.out.println(0);
        }
    }

    public static boolean isTriangleValid(int angleA, int angleB, int angleC) {
        // Check if all angles are positive and their sum is 180 degrees
        if (angleA > 0 && angleB > 0 && angleC > 0 && angleA + angleB + angleC == 180) {
            return true;
        }
        return false;
    }
}


